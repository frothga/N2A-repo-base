N2A.schema=1
$metadata
 backend.lems.part=tsodyksMarkramDepFacMechanism
 id=166b5f18573a90b2
 notes=Full Tsodyks-Markram STP model with both depression and facilitation, as in Tsodyks, Pawelzik and Markram 1998.
$up.plasticityFactor=*plasticityFactor
R
 $metadata
  notes=The spike event flag "in" comes from the containing synapse.
 @$init=1
 @in=R*(1-U)
R'=(1-R)/tauRec
U
 @$init=initReleaseProb
 @in=U+initReleaseProb*(1-U)
U'=(initReleaseProb-U)/tauFac
initReleaseProb=0
 $metadata
  public
plasticityFactor=:R*U
tauFac=0s
 $metadata
  public
tauRec=0s
 $metadata
  public
