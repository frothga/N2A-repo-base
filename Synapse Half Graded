N2A.schema=1
$inherit="Synapse Half Gap"
 0=166b5f185734fb2e
$metadata
 backend.lems.part=gradedSynapse
 id=166b5f185738e492
 notes=Analog synapse based on methods of http://www.nature.com/neuro/journal/v7/n12/abs/nn1352.html
E=0V
 $metadata
  backend.lems.param=erev
  notes=The reversal potential of the synapse
  public
I=:G*s*(E-V)
Vth=0V
 $metadata
  notes=The half-activation voltage of the synapse
  public
delta=0V
 $metadata
  notes=Slope of the activation curve
  public
inf=1/(1+exp((Vth-Vpeer)/delta))
k=0Hz
 $metadata
  notes=Rate constant for transmitter-receptor dissociation rate
  public
s=inf@1-inf<1e-4
s'=s_rate
s_rate=:
 @=0
 @1-inf>1e-4=(inf-s)/tau
tau=(1-inf)/k
