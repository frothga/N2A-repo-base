N2A.schema=1
$metadata
 backend.neuroml.part=gradedSynapse
 id=166b5f185738e492
 notes=Analog synapse based on methods of http://www.nature.com/neuro/journal/v7/n12/abs/nn1352.html
E=0V
 $metadata
  backend.neuroml.param=erev
  description=The reversal potential of the synapse
  public
G=0S
 $metadata
  backend.neuroml.param=conductance
  public
I=:G*s*(E-V)
 $metadata
  backend.neuroml.param=i
V=:0V
 $metadata
  backend.neuroml.param=v
Vpeer=:0V
 $metadata
  backend.neuroml.param=vpeer
Vth=0V
 $metadata
  description=The half-activation voltage of the synapse
  public
delta=0V
 $metadata
  description=Slope of the activation curve
  public
inf=1/(1+exp((Vth-Vpeer)/delta))
k=0Hz
 $metadata
  description=Rate constant for transmitter-receptor dissociation rate
  public
s=inf@1-inf<1e-4
s'=s_rate
s_rate=:
 @=0
 @1-inf>1e-4=(inf-s)/tau
tau=(1-inf)/k
