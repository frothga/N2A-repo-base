N2A.schema=1
$metadata
 id=166b015ccdaa5a31
 lock
A=$connect("Neuron")
B=$connect("Neuron")
B.I=+G*(E-B.V)
E=0mV
 $metadata
  backend.stpu.param
  note=Excitatory synapses have values near 0. Inhibitory synapses have values near B.Vrest
  public
Gmax=2uS
 $metadata
  backend.stpu.param
  public
W=1
 $metadata
  backend.stpu.param
event=:$event(A.V>=A.Vpeak)
