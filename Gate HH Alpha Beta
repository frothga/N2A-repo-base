$metadata
 backend.lems.part=gateHHrates
 id=166b5f185738c18d
 notes=Hodgkin-Huxley gate in which alpha and beta parameters imply both the time constant and steady-state value.
forwardRate
 $inherit="HHVariable"
  0=166b5f185735064a
 $up.alpha=x
inf=:alpha/(alpha+beta)@alpha+beta
q=inf@q==0
q'=(inf-q)/tau@tau
reverseRate
 $inherit="HHVariable"
  0=166b5f185735064a
 $up.beta=x
tau=:1/((alpha+beta)*Q10Scaling)
