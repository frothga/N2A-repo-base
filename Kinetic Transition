N2A.schema=1
$metadata
 backend.lems.part=KSTransition,forwardTransition,reverseTransition,tauInfTransition
 id=166b5f185734fe38
 notes=Specifies the rates of transition between two KS states.
forward=:inf/τ@τ
 $metadata
  backend.lems.param=rf
from=connect(Kinetic State)
from.occupancy'=+-reverse
inf=:0
reverse=:(1-inf)/τ@τ
 $metadata
  backend.lems.param=rr
to=connect(Kinetic State)
to.occupancy'=+forward
τ=:0s
 $metadata
  backend.lems.param=tau
