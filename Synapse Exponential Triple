N2A.schema=1
$inherit="Synapse Conductance"
 0=166b015ccdaa5a31
$metadata
 backend.lems.part=expThreeSynapse
 id=166b5f1857387284
G=:Gmax*(b-a)+Gmax2*(c-a)
Gmax=0uS
 $metadata
  backend.lems.param=gbase1
Gmax2=0uS
 $metadata
  backend.lems.param=gbase2
  public
Tpeak1=log(tauDecay1/tauRise)*(tauDecay1*tauRise)/(tauDecay1-tauRise)
 $metadata
  backend.lems.param=peakTime1
Tpeak2=log(tauDecay2/tauRise)*(tauDecay2*tauRise)/(tauDecay2-tauRise)
 $metadata
  backend.lems.param=peakTime2
a
 $metadata
  backend.lems.param=A
 @$init=0
 @in=a+weight*(Gmax*f1+Gmax2*f2)/(Gmax+Gmax2)
a'=-a/tauRise
 $metadata
  backend.lems.param=A'
b
 $metadata
  backend.lems.param=B
 @$init=0
 @in=b+weight*f1
b'=-b/tauDecay1
 $metadata
  backend.lems.param=B'
c
 $metadata
  backend.lems.param=C
 @$init=0
 @in=c+weight*f2
c'=-c/tauDecay2
 $metadata
  backend.lems.param=C'
f1=1/(exp(-Tpeak1/tauDecay1)-exp(-Tpeak1/tauRise))
 $metadata
  backend.lems.param=waveformFactor1
f2=1/(exp(-Tpeak2/tauDecay2)-exp(-Tpeak2/tauRise))
 $metadata
  backend.lems.param=waveformFactor2
tauDecay1=0s
 $metadata
  public
tauDecay2=0s
 $metadata
  public
tauRise=0s
 $metadata
  public
weight=1
